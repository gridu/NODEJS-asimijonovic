{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "env-cmd -f ./config/dev.env nodemon --ext js,graphql server.js",
    "start:prod": "env-cmd -f ./config/prod.env NODE_ENV=production nodemon server.js",
    "debug": "env-cmd -f ./config/dev.env ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "test": "env-cmd -f ./config/test.env jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/jest/jest.setup.js"
    ],
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js",
    "testEnvironment": "node"
  },
  "author": "Jonas Schmedtmann",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^2.13.1",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^3.5.0",
    "graphql-middleware": "^4.0.2",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.16.0",
    "hpp": "^0.2.2",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.1.1",
    "pug": "^3.0.1",
    "sharp": "^0.22.1",
    "slugify": "^1.3.4",
    "stripe": "^7.0.0",
    "validator": "^10.11.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "apollo-link-http": "^1.5.17",
    "cross-fetch": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "sinon": "^9.0.2"
  },
  "engines": {
    "node": "^10"
  }
}
